  <brief_title>Loteprednol Etabonate Versus Vehicle for the Treatment of Inflammation and Pain Following Cataract Surgery</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Bausch &amp; Lomb Incorporated</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
