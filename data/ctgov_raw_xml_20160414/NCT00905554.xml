  <brief_title>Warm Water for Unsedated Colonoscopy</brief_title>
  <acronym>WW</acronym>
  <official_title>Warm Water Irrigation Versus Standard Air Insufflation for Unsedated Colonoscopy: a Randomized Controlled Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Valduce Hospital</agency>
      <agency_class>Other</agency_class>
