  <brief_title>Evaluation of Screening Methods for Treatment of Meibomian Gland Dysfunction</brief_title>
  <official_title>Pilot Study for Evaluation of LipiFlow Screening Methods</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>TearScience, Inc.</agency>
      <agency_class>Industry</agency_class>
