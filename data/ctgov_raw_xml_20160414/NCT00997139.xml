  <brief_title>Clearance of Nasal Staphylococcus Aureus With Triple Antibiotic Ointment</brief_title>
  <official_title>Clearance of Nasal Staphylococcus Aureus Colonization With Triple Antibiotic Ointment</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Northwestern University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
