  <primary_outcome>
    <measure>Global Ocular Staining (With Oxford Scale - Ranges : 0-15)</measure>
    <time_frame>Baseline and Day 35</time_frame>
    <safety_issue>No</safety_issue>
    <description>Change from Baseline in the worse eye on Day 35 (decrease of Oxford score = better outcome)</description>
  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">105</enrollment>
