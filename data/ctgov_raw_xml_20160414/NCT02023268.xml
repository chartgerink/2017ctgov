    <has_dmc>No</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      Comparison of the efficacy and safety of T2762 and Vismed® in the treatment of moderate to
      severe Dry Eye Syndrome
    </textblock>
  </brief_summary>
  <overall_status>Completed</overall_status>
  <start_date>December 2013</start_date>
  <completion_date type="Actual">February 2015</completion_date>
  <primary_completion_date type="Actual">December 2014</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
