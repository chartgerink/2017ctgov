                <counts group_id="E1" events="1" subjects_affected="1" subjects_at_risk="22"/>
              </event>
              <event>
                <sub_title>psuedomonas sepsis, gr 5</sub_title>
                <counts group_id="E1" events="1" subjects_affected="1" subjects_at_risk="22"/>
              </event>
            </event_list>
          </category>
          <category>
            <title>Metabolism and nutrition disorders</title>
            <event_list>
              <event>
                <sub_title>Hyponatremia, gr 3</sub_title>
