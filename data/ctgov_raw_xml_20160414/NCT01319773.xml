  <brief_title>Study of Two Formulations of Cyclosporine Ophthalmic Emulsion in Healthy Volunteers and in Patients With Dry Eye</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
