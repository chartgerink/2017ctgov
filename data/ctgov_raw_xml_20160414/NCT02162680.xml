  <brief_title>An Evaluation of the Effectiveness of Local Anesthetics Used During Intravenous Catheter Insertion</brief_title>
  <official_title>An Evaluation of the Effectiveness of Local Anesthetics Used During Intravenous Catheter Insertion</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Duke University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
