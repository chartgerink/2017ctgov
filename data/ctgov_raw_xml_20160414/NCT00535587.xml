  <brief_title>Testing Mestinon and Exercise in Fibromyalgia</brief_title>
  <official_title>Maximizing Exercise Effectiveness in Fibromyalgia</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Oregon Health and Science University</agency>
      <agency_class>Other</agency_class>
