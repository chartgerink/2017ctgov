            <category_list>
              <category>
                <measurement_list>
                  <measurement group_id="O1" value="5.6" lower_limit="0" upper_limit="16.6"/>
                  <measurement group_id="O2" value="27.2" lower_limit="0" upper_limit="55.1"/>
