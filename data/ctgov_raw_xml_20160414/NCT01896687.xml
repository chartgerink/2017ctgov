  <brief_title>Trial of Scrambler Therapy or Sham Treatment for Low Back Pain</brief_title>
  <official_title>A Pilot Randomized Trial of Scrambler Therapy or Sham Treatment for Persistent Nonspecific Low Back Pain</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Virginia Commonwealth University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
