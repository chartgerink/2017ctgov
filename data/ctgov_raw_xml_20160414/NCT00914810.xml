  <brief_title>Effect of Supplemental Vitamin D on Skeletal Muscle Function in Chronic Obstructive Pulmonary Disease (COPD) Patients</brief_title>
  <official_title>Effect of Supplemental Vitamin D on Skeletal Muscle Function in COPD Patients.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Minneapolis Veterans Affairs Medical Center</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
  </sponsors>
