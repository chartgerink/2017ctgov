  <brief_title>Lidocaine Patches Prior to Intravenous Insertion</brief_title>
  <official_title>Phase II Study of Lidoderm Patches Prior to Intravenous Catheter Insertion.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Stony Brook University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
