  <brief_title>Test of Discomfort and Malaise of Two Different Urine Catheters in Healthy Volunteers</brief_title>
  <official_title>Comparative Study of Two Different Urine Catheters.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Coloplast A/S</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
