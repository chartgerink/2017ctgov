  <brief_title>A Study of Ketorolac for the Treatment of Inflammation and Pain Associated With Cataract Surgery</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
