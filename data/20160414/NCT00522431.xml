  <brief_title>A Study of Fortigel Testosterone Gel 2% in Males With Low Testosterone</brief_title>
  <official_title>An Open Label Phase 3 Study of Fortigel Testosterone Gel 2% in Hypogonadal Males</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Prostrakan Pharmaceuticals</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
