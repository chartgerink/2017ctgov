  <brief_title>FREEDOM - A Frequent Optimization Study Using the QuickOpt Method</brief_title>
  <official_title>FREEDOM - A Frequent Optimization Study Using the QuickOpt Method</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>St. Jude Medical</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
