  <brief_title>Oral Progesterone for Prevention of Preterm Birth</brief_title>
  <official_title>Prevention of Recurrent Preterm Birth With Micronized Progesterone</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Fetal Medicine Foundation</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
