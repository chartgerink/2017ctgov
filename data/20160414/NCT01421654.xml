  <brief_title>Evaluation of Acclimate Feature for Positive Airway Pressure (PAP) Therapy</brief_title>
  <official_title>Evaluation of Positive Airway Pressure (PAP) Feature to Improve Patient Adherence to Therapy</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>ResMed</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
