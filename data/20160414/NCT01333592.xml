  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Type 2 diabetes patients who show inadequate glycemic control with diet, and
             biguanide or DPP-4 inhibitor monotherapy

          -  Age in the 20 years or over inclusive

          -  HbA1c in the range of ≥ 6.5 to &lt; 9%

        Exclusion Criteria:

          -  Type 1 diabetes mellitus

          -  Patients with serious diabetic complications and other serious complications
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
