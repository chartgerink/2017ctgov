  <brief_title>Drug Combination on Exercise Performance at High Altitude</brief_title>
  <official_title>Enhancing Physical Performance and Mitigating Acute Mountain Sickness Via Pharmaceutical Intervention While at Altitude</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Montana</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
