  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Age 8-18 years, referred from emergency department for suspected appendicitis and
             receiving either CT scan or ultrasound of the abdomen for diagnosis

        Exclusion Criteria:

          -  Failure to pass MRI metal screening. Claustrophobia or need for sedation due to
             inability to hold still.
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>8 Years</minimum_age>
    <maximum_age>18 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
