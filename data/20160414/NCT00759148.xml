  <brief_title>Moxifloxacin AF Ophthalmic Solution for Treatment of Bacterial Conjunctivitis</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Alcon Research</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
