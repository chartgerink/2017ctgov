  <brief_title>Comparison of Prilosec OTC® Versus Zegerid® for Gastric Acid Suppression</brief_title>
  <official_title>Comparison of Prilosec OTC® Versus Zegerid® for Gastric Acid Suppression</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Procter and Gamble</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Procter and Gamble</source>
